{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**", "package.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "dist/main.ts"
    }
  ]
}
